}

void saveTabGroup(String tabgroup, String callback) {
  Boolean enableAutosave = true;
  String  uuid           = getUuid(tabgroup);

  SaveCallback saveCallback = getDefaultSaveCallback(tabgroup, callback);

  saveTabGroup(
      tabgroup,
      uuid,
      autoSaveGeo,
      autoSaveAttrs,
      saveCallback,
      enableAutosave
  );

  // Reset global variables
  autoSaveGeo   = null;
  autoSaveAttrs = null;
}

